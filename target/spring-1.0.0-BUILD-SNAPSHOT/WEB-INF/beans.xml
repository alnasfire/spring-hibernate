<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:mvc="http://www.springframework.org/schema/mvc"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="
http://www.springframework.org/schema/mvc http://www.springframework.org/schema/mvc/spring-mvc-3.0.xsd
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd"
       xmlns:aop="http://www.springframework.org/schema/aop">

    <bean id="registerUser" class="com.ctp.spring.controller.RegisterUserController"/>
    <bean id="home" class="com.ctp.spring.controller.HomeController"/>
    <bean id="testslist" class="com.ctp.spring.controller.TestsListController"/>
    <bean id="test" class="com.ctp.spring.controller.TestController"/>
    <bean id="testresult" class="com.ctp.spring.controller.TestResultController"/>
    <bean id="passedtest" class="com.ctp.spring.controller.PassedTestController"/>
    <bean id="createtest" class="com.ctp.spring.controller.CreateTestController"/>
</beans>